{
  "version": "3.0.0-alpha-*",
  "authors": [
    "Shay Rojansky",
    "Emil Lenngren",
    "Francisco Figueiredo Jr.",
    "Kenji Uno",
    "Jon Asher",
    "Josh Cooley",
    "Federico Di Gregorio",
