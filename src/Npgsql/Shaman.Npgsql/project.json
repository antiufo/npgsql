{
  "version": "3.0.1.2",
  "authors": [
    "Shay Rojansky",
    "Emil Lenngren",
    "Francisco Figueiredo Jr.",
    "Kenji Uno",
    "Jon Asher",
    "Josh Cooley",
    "Federico Di Gregorio",
    "Jon Hanna",
    "Chris Morgan",
    "Dave Page",
    "Glen Parker",
    "Brar Piening",
    "Hiroshi Saito"
  ],
  "packOptions": {
    "iconUrl": "http://www.npgsql.org/img/postgresql.gif",
    "owners": [
      "Shay Rojansky",
      "Emil Lenngren",
      "Francisco Figueiredo Jr.",
      "Kenji Uno"
    ]
  },
  "description": "Npgsql is a .NET data provider for PostgreSQL, 100% implemented in C# code. PostgreSQL 9.x and above is officially supported, other versions may work.",
  "buildOptions": {
    "warningsAsErrors": true,
    "define": [
      "__MonoCS__"
    ],
    "keyFile": "../../../Npgsql.snk",
    "compile": {
      "include": [
        "../**/*.cs",
        "../../Shared/*.cs"
      ]
    }
  },
  "s---cripts": {
    "prebuild": [
      "cd .. && dotnet rewrite-async"
    ]
  },
  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [
          "NET45"
        ]
      },
      "frameworkAssemblies": {
        "System.Data": "4.0",
        "System.Transactions": "4.0",
        "System.DirectoryServices": "4.0",
        "System.Numerics": "4.0",
        "System.Xml": "4.0"
      },
      "dependencies": {
        "Shaman.SystemReimpl": "1.0.0-beta-2555"
      }
    },
    "netstandard1.3": {
      "buildOptions": {
        "define": [
          "CORECLR",
          "DNXCORE50"
        ]
      },
      "imports": [
        "portable-net45+win8"
      ],
      "dependencies": {
        "System.Collections.Concurrent": "4.0.12",
        "System.Console": "4.0.0",
        "System.ComponentModel": "4.0.1",
        "System.ComponentModel.TypeConverter": "4.1.0",
        "System.Data.Common": "4.1.0",
        "System.Diagnostics.Contracts": "4.0.1",
        "System.Diagnostics.Tools": "4.0.1",
        "System.Diagnostics.StackTrace": "4.0.2",
        "System.Globalization": "4.0.11",
        "System.Globalization.Extensions": "4.0.1",
        "System.Linq": "4.1.0",
        "System.Net.NameResolution": "4.0.0",
        "System.Net.Primitives": "4.0.11",
        "System.Net.Sockets": "4.1.0",
        "System.Net.Security": "4.0.0",
        "System.Net.NetworkInformation": "4.1.0",
        "System.Runtime": "4.1",
        "System.Runtime.Extensions": "4.1.0",
        "System.Runtime.InteropServices": "4.1.0",
        "System.Runtime.Numerics": "4.0.1",
        "System.Reflection": "4.1.0",
        "System.Reflection.Primitives": "4.0.1",
        "System.Reflection.Extensions": "4.0.1",
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Security.Cryptography.Primitives": "4.0.0",
        "System.Security.Cryptography.Algorithms": "4.2.0",
        "System.Security.Cryptography.X509Certificates": "4.1.0",
        "System.Security.Principal": "4.0.1",
        "System.Text.Encoding.Extensions": "4.0.11",
        "System.Text.RegularExpressions": "4.1.0",
        "System.Threading": "4.0.11",
        "System.Threading.Thread": "4.0.0",
        "System.Threading.Timer": "4.0.1",
        "System.Resources.ResourceManager": "4.0.1",
        "System.Diagnostics.Debug": "4.0.11"
      }
    }
  }
}